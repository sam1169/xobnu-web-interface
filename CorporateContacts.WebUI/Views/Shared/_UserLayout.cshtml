<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <link href="~/Content/Site.css" type="text/css" rel="stylesheet" />
    <link href="~/Content/Menu.css" type="text/css" rel="stylesheet" />
    <script src="~/Scripts/jquery-1.8.2.js"></script>
    <script src="~/Scripts/jquery.validate.min.js"></script>
    <script src="~/Scripts/jquery.validate.unobtrusive.min.js"></script>
</head>
<body>
    <div id="header">
        <div class="title"><a style="color:white" href="~/">Corporate Contacts</a></div>
        <div style="text-align: right; margin: 0px 20px">Logged in as @(((Xobnu.Domain.Entities.User)Session["user"]).FullName)(<a style="color: #fff; text-decoration: underline;" href="~/user/signout">signout</a>)</div>
        <ul id="menu">
            <li class="@(ViewBag.SelectedMenu == "SetupSync" ? "selected" : null)"><a href="~/User/SetupSync">Setup Sync</a></li>
        </ul>
    </div>

    <div id="content">

        @RenderBody()
    </div>

    
</body>
</html>
